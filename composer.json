{
    "name": "team-reflex/challonge-php",
    "description": "PSR-18 compatible library to interface with the Challonge API v2.1 with OAuth support.",
    "require": {
        "ext-json": "*",
        "php": "^8.1 || ^8.2 || ^8.3 || ^8.4",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/log": "^3.0",
        "illuminate/collections": "^9.0 || ^10.0 || ^11.0 || ^12.0",
        "cuyz/valinor": "^1.14",
        "nyholm/psr7": "^1.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5 || ^11.0",
        "php-http/mock-client": "^1.6",
        "symfony/http-client": "^6.0 || ^7.0",
        "guzzlehttp/guzzle": "^7.8",
        "phpstan/phpstan": "^1.10",
        "laravel/pint": "^1.13"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Kyle Ward",
            "email": "kairu@team-reflex.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Reflex\\Challonge\\": "src/Challonge"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
